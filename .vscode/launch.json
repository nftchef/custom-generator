{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Main",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Rarity Util",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/utils/rarityData.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Regenerate",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "4",
        "-d"
      ],
      "program": "${workspaceFolder}/utils/regenerate.js"
    }
  ]
}